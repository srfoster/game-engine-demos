#lang racket

(provide spaceship-sprite)

(require "../../game-engine.rkt")

(define spaceship-sheet (bitmap/url "http://i.imgur.com/8zY5sBR.png"))
(define spaceship-sprite
  (sheet->sprite spaceship-sheet
                 #:rows        4
                 #:columns     3
                 #:row-number  3
                 #:speed       1))

